{
  
    "functions": {
      "api/index.mjs": {
        "memory": 128,
        "maxDuration": 5
      },
      "api/*.js": {
        "memory": 128,
        "maxDuration": 5
      }
    }
  ,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "api/index.mjs"
    }
  ],
    "headers": [
    {
      "source": "/",
      "headers" : [
        {
          "key" : "Content-Type",
          "value" : "text/html"
        },
        {
          "key" : "Access-Control-Allow-Origin",
          "value" : "*"
        }
      ]
    }
  ]
}
